
<ol id='links'>
  <% @links.each do |link| %>
    <li>  Title: <%= link.title %>
      URL:   <%= link.url %>
  Tags:  <% link.tags.sort.each do |tag| %>
       <%= tag.name %> |
    <% end %>
  </ul>
    </li>
  <% end %>
</ol>


<form action="/new">
